Private sub btnshowAnswer_click(sender As Object, e As EventArgs) Handles btnshowAnswer.Click
	‘btnShowAnswer click isn't turns lblUnsolved Invisible and turn lblAnswer to Visible
	lblAnswer.Visible = True
	lblUnsolved.Visible = True
End Sub

Private sub btnClose_click(sender As Object, e As EventArgs) Handles btnClose.Click
	‘ Close application when btnClose it's clicked
	Close()
End Sub

End Class
